import "lib/sql" (SqlDB, SqlTx, sqlOpen, sqlClose)
import "lib/date" (Date)
import "lib/test" (assert, testRun)

testSqlTypesExport = fun() {
    // Check explicit import of SqlDB type (usage only)

    // Check Date type from lib/date (which is used by lib/sql)
    d: Date = {
        year: 2023, month: 1, day: 1,
        hour: 0, minute: 0, second: 0,
        offset: 0
    }

    assert(d.year == 2023, "Date type usage")
}

testRun("sql types export", testSqlTypesExport)
