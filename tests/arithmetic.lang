// Int arithmetic
print(10 + 2)
print(10 - 2)
print(10 * 2)
print(10 / 2)
print(2 ** 3)

// Modulo (Int)
print(17 % 5)
print(10 % 3)
print(-7 % 3)

// Float arithmetic
print(1.5 + 0.5)
print(3.0 * 2.0)

// Modulo (Float)
print(10.5 % 3.0)

// BigInt arithmetic
print(100n + 50n)
print(100n * 2n)
print(17n % 5n)

// Rational arithmetic
print(1r + 1r)
print(3r / 2r)
print(7r % 3r)

// Bitwise
print(1 & 3)
print(1 | 2)
print(1 ^ 3)
print(1 << 1)
print(4 >> 1)

// Formats
print(0xFF)
print(0b101)
