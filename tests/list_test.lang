l = [1, 2, 3]
print(l) // [1, 2, 3]

s = "hello"
print(s) // "hello"

c = 'x'
print(c) // 'x'

// Indexing
print(l[0]) // 1
print(l[-1]) // 3
print(s[1]) // 'e'
print(s[-1]) // 'o'

// List of chars as string
ls = ['a', 'b', 'c']
print(ls) // "abc"

// Nested lists
nested = [[1, 2], [3, 4]]
print(nested[0][1]) // 2

